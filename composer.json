{
    "name": "kaely/pos-customer",
    "description": "Laravel package for managing POS customers with billing, shipping addresses, purchase history, and loyalty points",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "pos",
        "customer",
        "billing",
        "shipping",
        "loyalty",
        "points",
        "credit"
    ],
    "authors": [
        {
            "name": "Kaely Team",
            "email": "dev@kaely.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "kaelytechnology/auth-package": "^1.0",
        "kaely/pos-sale": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kaely\\PosCustomer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaely\\PosCustomer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaely\\PosCustomer\\PosCustomerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 